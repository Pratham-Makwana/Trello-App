{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "typeRoots": ["node_modules/@types","src/types"],
    "types": ["jest"],
    "baseUrl": "./src/",
    "paths": {
      "@assets/*":["assets/*"],
      "@features/*":["features/*"],
      "@service/*":["service/*"],
      "@styles/*":["styles/*"],
      "@navigation/*":["navigation/*"],
      "@components/*":["components/*"],
      "@state/*":["state/*"],
      "@utils/*":["utils/*"],
      "@context/*":["context/*"],
      "@config/*" : ["config/*"],
      "@store/*": ["store/*"],
      "@hooks/*": ["hooks/*"],
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"]
    }
  }
}
